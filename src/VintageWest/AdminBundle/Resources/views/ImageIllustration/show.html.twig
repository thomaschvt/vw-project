{% extends '::admin_base.html.twig' %}

{% block body -%}
    <h1>ImageIllustration</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Titlefr</th>
                <td>{{ entity.titleFr }}</td>
            </tr>
            <tr>
                <th>Titleen</th>
                <td>{{ entity.titleEn }}</td>
            </tr>
            <tr>
                <th>Titlees</th>
                <td>{{ entity.titleEs }}</td>
            </tr>
            <tr>
                <th>Altfr</th>
                <td>{{ entity.altFr }}</td>
            </tr>
            <tr>
                <th>Alten</th>
                <td>{{ entity.altEn }}</td>
            </tr>
            <tr>
                <th>Altes</th>
                <td>{{ entity.altEs }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ entity.url }}</td>
            </tr>
            <tr>
                <th>Isincarrousel</th>
                <td>{{ entity.isInCarrousel }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('imageillustration') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('imageillustration_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
